[main]
server                    = <%= puppet_server %>
logdir                    = <%= puppet_logdir %>
vardir                    = <%= puppet_vardir %>
ssldir                    = <%= puppet_ssldir %>
rundir                    = /var/run/puppet
factpath                  = $vardir/lib/facter
pluginsync                = true
templatedir               = $confdir/templates

[agent]
report                    = true
environment               = production
