LoadPlugin snmp

<Plugin "snmp">

  <Data "dell_temperature">
    Type "temperature"
    Table true
    Instance "SNMPv2-SMI::enterprises.674.10892.1.700.20.1.8.1"
    Values "SNMPv2-SMI::enterprises.674.10892.1.700.20.1.6.1"
    Scale 0.1
  </Data>

  <Data "dell_power">
    Type "power"
    Table true
    Instance "SNMPv2-SMI::enterprises.674.10892.1.600.30.1.8.1"
    Values "SNMPv2-SMI::enterprises.674.10892.1.600.30.1.6.1"
  </Data>

  <Host "<%= @fqdn %>">
    Address "127.0.0.1"
    Version 1
    Community "public"
    Collect "dell_temperature" "dell_power"
    Interval 120
  </Host>

</Plugin>
