#User alias specification
User_Alias IOTDEV = dbush, ttai, rli4, rhage, pwarner, pbarrett, ppeng, zliu1 ,ywang, jshi, bli3, aning, dkhoerni, ezhang

# Cmnd alias specification
Cmnd_Alias IOT_COMMANDS = /bin/mount, /bin/umount, /sbin/losetup, /bin/cp, /usr/bin/find

IOTDEV  ALL = IOT_COMMANDS

IOTDEV  ALL=(ALL:ALL) NOPASSWD:IOT_COMMANDS